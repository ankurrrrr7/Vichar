{
    "version": 2,
    "builds": [
      {
        "src": "frontend/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
        {
          "src": "/backend/src/main.js",
          "dest": "/backend/src/main.js"
        },
        {
          "src": "/(.*)",
          "dest": "/frontend/$1"
        }
      ]
  }


  